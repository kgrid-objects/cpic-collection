{
  "name": "cpic_kit",
  "version": "1.0.0",
  "description": "CPIC KGrid Kit",
  "main": " ",
  "scripts": {
    "postinstall": "chmod +x scripts/* && node scripts/clean.js && node scripts/kgridInstall.js",
    "clean": "node scripts/clean.js",
    "start": "scripty",
    "stop": "scripty",
    "package": "node scripts/package.js"
  },
  "author": "KGrid Team",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.11",
    "commander": "^2.17.1",
    "download": "^7.1.0",
    "file-exists": "^5.0.1",
    "fs-extra": "^7.0.0",
    "http-server": "^0.11.1",
    "jsonpath": "^1.0.0",
    "request": "^2.88.0",
    "scripty": "^1.7.2",
    "shelljs": "^0.8.2",
    "unzip": "^0.1.11"
  },
  "scripty": {
    "path": "scripts",
    "windowsPath": "scriptswin",
    "silent": true
  },
  "githubAssets": {
    "library": {
      "url": "https://api.github.com/repos/kgrid/kgrid-library",
      "destination": "library"
    },
    "activator": {
      "url": "https://api.github.com/repos/kgrid/kgrid-activator",
      "destination": "activator"
    },
    "cpic-kos": {
      "url": "https://api.github.com/repos/kgrid-objects/cpic-objects",
      "destination": "dist"
    }
  }
}
